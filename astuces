-- replace coordinate123 -> coordinate --
coord_1_in_vox="(\d+\.?\d+)",\s*coord_2_in_vox="(\d+\.?\d+)",\s*coord_3_in_vox="(\d+\.?\d+)"
coord_in_vox="[$1, $2, $3]"

coord_1="(-?\d?\.?\d+)",\s*coord_2="(-?\d?\.?\d+)",\s*coord_3="(-?\d?\.?\d+)"
coord="[$1, $2, $3]"

coord([^\s]*)="\[([^\s]*)\s*,\s*([^\s]*)\s*,\s*([^\s]*)\s*]"
coord$1="[$2, $3, $4]"

